{
  "name": "moned-full",
  "version": "0.2.0-dev",
  "private": true,
  "type": "module",
  "main": "./dist/moned-full.js",
  "module": "./dist/moned-full.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/moned-full.js",
      "types": "./dist/src/index.d.ts"
    },
    "./moned-workers": {
      "import": "./dist/moned-workers.js"
    },
    "./moned-workers-vite": {
      "import": "./dist/moned-workers-vite.js"
    }
  },
  "files": [
    "dist/assets",
    "dist/src",
    "dist/moned*.js",
    "dist/style.css",
    "src"
  ],
  "scripts": {
    "prepare": "npm run _build-workers",
    "clean": "rimraf ./dist",
    "dev": "vite",
    "lint": "eslint src",
    "_build-workers": "vite -c vite.config.worker.ts build",
    "_build-main": "tsc && vite build",
    "build": "npm run clean && npm run _build-main && npm run _build-workers",
    "serve": "tsc && vite preview"
  },
  "dependencies": {
    "monaco-editor": "^0.32.1"
  },
  "devDependencies": {
    "tslib": "2.3.1"
  }
}
