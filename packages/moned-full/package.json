{
  "name": "moned-full",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/moned-full.es.js",
  "module": "./dist/moned-full.es.js",
  "exports": {
    ".": {
      "import": "./dist/moned-full.es.js"
    },
    "./moned-workers": {
      "import": "./dist/moned-workers.es.js"
    },
    "./moned-workers-vite": {
      "import": "./dist/moned-workers-vite.es.js"
    }
  },
  "files": [ "dist" ],
  "scripts": {
    "prepare": "npm run clean && npm run _build-workers",
    "clean": "rimraf ./dist",
    "dev": "vite",
    "lint": "eslint src",
    "_build-workers": "vite -c vite.config.worker.ts build",
    "_build-main": "vite build",
    "build": "npm run clean && tsc && npm run _build-main && npm run _build-workers",
    "serve": "tsc && vite preview"
  },
  "dependencies": {
    "monaco-editor": "^0.32.1"
  }
}
