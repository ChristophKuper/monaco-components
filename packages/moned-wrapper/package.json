{
  "name": "moned-wrapper",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/main.es.js",
  "module": "./dist/main.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "types": "./dist/main.d.ts"
    }
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/*.css",
    "dist/assets/*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "vite",
    "lint": "eslint src",
    "build": "npm run clean && tsc && vite build ",
    "serve": "tsc && vite preview",
    "update-verify": "cp-cli ./dist ./verify",
    "verify": "npm run update-verify && http-server ./verify"
  },
  "dependencies": {
    "monaco-editor": "^0.31.1",
    "http-server": "14.1.0",
    "cp-cli": "^2.0.0"
  }
}
