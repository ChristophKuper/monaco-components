{
  "name": "monaco-editor-lc-comp",
  "version": "0.3.0",
  "license": "MIT",
  "private": "true",
  "description": "Lit Component for Monaco Languageclient",
  "keywords": [
    "monaco-editor",
    "monaco-languageclient",
    "lit",
    "web-component",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "shx rm -rf ./dist && shx rm -fr tsconfig.tsbuildinfo",
    "lint": "eslint src",
    "compile": "tsc",
    "build": "npm run clean && npm run compile"
  },
  "dependencies": {
    "monaco-editor": "0.33.0",
    "monaco-editor-workers": "../monaco-editor-workers",
    "monaco-languageclient": "^0.19.0-next.0",
    "@codingame/monaco-jsonrpc": "^0.3.1",
    "lit": "^2.2.1",
    "normalize-url": "^7.0.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.66.0",
    "@types/glob-to-regexp": "^0.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeFox/monaco-components",
    "directory": "packages/monaco-editor-lc-comp"
  },
  "homepage": "https://github.com/TypeFox/monaco-components/blob/main/packages/monaco-editor-lc-comp/README.md",
  "bugs": "https://github.com/TypeFox/monaco-components/issues",
  "author": {
    "name": "TypeFox",
    "url": "https://www.typefox.io"
  }
}
